palidromic substring

class Solution {
public:
    int countSubstrings(string s) {
        int ret = 0;
        bool dp[s.size()][s.size()];
        
        dp[0][0] = true;
        
        for(int i = 0; i < s.size(); i++) {
            for(int j = 0; j <= i; j++) {
                if((s[j] == s[i]) && (i - j <= 2 || dp[j + 1][i - 1])) {
                    ret++;
                }
            }
        }
        return ret;
    }
};
